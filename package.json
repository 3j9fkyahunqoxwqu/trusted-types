{
  "name": "trusted-types",
  "version": "1.0.0",
  "description": "Polyfill for the Trusted Types",
  "main": "dist/cjs/trustedtypes.api_only.js",
  "directories": {
    "test": "tests"
  },
  "contributors": [
    "Krzysztof Kotowicz <koto@google.com>",
    "Sebastian Lekies <slekies@google.com>"
  ],
  "scripts": {
    "lint": "eslint src/ tests/",
    "fix": "eslint --fix src/ tests/",
    "test": "karma start --single-run",
    "karma": "karma start --browsers ChromeHeadless",
    "build": "run-p build:cjs build:es6 build:es5",
    "build:cjs": "rollup -c",
    "build:es5": "gulp es5",
    "build:es6": "gulp es6",
    "add_build": "git add ./dist/"
  },
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "google-closure-compiler": "^20180506.0.0",
    "gulp": "^3.9.1",
    "gulp-closure-compiler": "^0.4.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.2",
    "karma-browserify": "^5.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "rollup": "^0.60.4",
    "watchify": "^3.11.0"
  },
  "dependencies": {},
  "pre-commit": [
    "lint",
    "test",
    "build",
    "add_build"
  ],
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "LICENSE",
    "README.md"
  ]
